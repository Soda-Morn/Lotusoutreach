# Ignore system files
.DS_Store
Thumbs.db

# Node modules (Vue.js)
frontend/node_modules/
frontend/dist/

# Laravel vendor folder & caches
backend/vendor/
backend/storage/*.key
backend/storage/logs/
backend/storage/framework/cache/
backend/storage/framework/sessions/
backend/storage/framework/views/

# Laravel environment & cache
backend/.env
backend/.env.*
backend/.phpunit.result.cache

# Laravel IDE helper (optional)
backend/_ide_helper.php
backend/_ide_helper_models.php
backend/.phpstorm.meta.php/

# npm debug logs
frontend/npm-debug.log*
frontend/yarn-debug.log*
frontend/yarn-error.log*

# Vite cache (if using Vite)
frontend/.vite/
backend/.vite/

# Composer lock file (optional)
# backend/composer.lock

# Node lock file (optional)
# frontend/package-lock.json
# frontend/yarn.lock
